---
processJSON([
  {% for post in site.posts %}
    {
      "guid": "{{ post.guid }}",
      "title": "{{ post.title }}",
      "latitude": "{{ post.latitude }}",
      "longitude": "{{ post.longitude }}",
      "url": "{{ post.link }}",
      "thumbnail": "{{ post.thumbnailUrl }}"
    }
    {% unless forloop.last %},{% endunless %}
  {% endfor %}
]);
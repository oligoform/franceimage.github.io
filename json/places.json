{% assign posts = site.posts | where:"type", "places" %}
processJSON([
  {% for post in posts %}
    {
      "guid": "{{ post.guid }}",
      "title": "{{ post.title }}",
      "latitude": "{{ post.latitude }}",
      "longitude": "{{ post.longitude }}",
      "youtubeId": "{{ post.youtubeId }}"
    }
    {% unless forloop.last %},{% endunless %}
  {% endfor %}
]);
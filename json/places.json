{% assign posts = site.posts | where:"type", "places" %}
processJSON([
  {% for post in posts %}
    {
      "guid": "{{ post.guid }}",
      "title": "{{ post.title }}",
      "latlng": "{{ post.latlng }}",
      "youtubeId": "{{ post.youtubeId }}"
    }
    {% unless forloop.last %},{% endunless %}
  {% endfor %}
]);